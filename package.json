{
  "name": "aurelia-plugins-tabs",
  "version": "2.5.0",
  "description": "A tabs plugin for Aurelia.",
  "keywords": [
    "aurelia",
    "plugins",
    "plugin",
    "tabs"
  ],
  "homepage": "https://github.com/aurelia-plugins/aurelia-plugins-tabs",
  "license": "MIT",
  "author": "Bart Lenaerts <bart@itprosolutions.be> (http://itprosolutions.be/)",
  "bugs": {
    "url": "https://github.com/aurelia-plugins/aurelia-plugins-tabs/issues"
  },
  "main": "dist/commonjs/aurelia-plugins-tabs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/aurelia-plugins/aurelia-plugins-tabs.git"
  },
  "dependencies": {
    "aurelia-dependency-injection": "^1.3.1",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-pal": "^1.3.0",
    "aurelia-templating": "^1.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-typescript": "^3.1.7",
    "run-sequence": "^1.2.2",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "build": "gulp",
    "clean": "rm -rf node_modules && yarn"
  },
  "jspm": {
    "registry": "npm",
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "dependencies": {
      "aurelia-dependency-injection": "^1.3.1",
      "aurelia-event-aggregator": "^1.0.1",
      "aurelia-pal": "^1.3.0",
      "aurelia-templating": "^1.4.2"
    }
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-plugins-tabs/aurelia-plugins-tabs-tabs",
        "aurelia-plugins-tabs/aurelia-plugins-tabs-tab-content",
        "aurelia-plugins-tabs/aurelia-plugins-tabs-tab-pane"
      ]
    }
  }
}